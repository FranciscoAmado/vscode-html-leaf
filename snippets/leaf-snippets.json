{
    "raw": {
        "prefix": "#raw",
        "description": "Raw block",
        "body": [
            "#raw() {",
            "\t$0",
            "}"
        ]
    },
    "tag": {
        "prefix": "#tag",
        "description": "Empty tag",
        "body": [
            "#($1)"
        ]
    },
    "for loop": {
        "prefix": "#loop",
        "description": "For loop",
        "body": [
            "#loop(${array}, \"${string}\") {",
            "\t$0",
            "}"
        ]
    },
    "if-else condition": {
        "prefix": "#if-else",
        "description": "If-else condition",
        "body": [
            "#if(${boolean}) {",
            "\t$0",
            "} ##else() {",
            "\t$0",
            "}"
        ]
    },
    "import leaf": {
        "prefix": "#import",
        "description": "Import an exported Leaf block onto here",
        "body": [
            "#import(\"${string}\")"
        ]
    },
    "export leaf": {
        "prefix": "#export",
        "description": "Export a Leaf block",
        "body": [
            "#export(\"${string}\") {",
            "\t$0",
            "}"
        ]
    },
    "extend leaf": {
        "prefix": "#extend",
        "description": "Extend from other Leaf file",
        "body": [
            "#extend(\"${string}\")"
        ]
    },
    "embed leaf": {
        "prefix": "#embed",
        "description": "Embed leaf into your tree",
        "body": [
            "#embed(\"${string}\") {",
            "\t$0",
            "}"
        ]
    }
}
